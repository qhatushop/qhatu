          "_name": "extPricingTables10",
          "_customHTML": "<section class=\"extPricingTables\" group=\"Additional Blocks\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"6\">\n        <input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Card Color\" name=\"cardColor\" value=\"#00e29e\">\n        <input type=\"color\" title=\"Card Hover\" name=\"cardHover\" value=\"#a638bd\">\n        <select title=\"Items Count\" name=\"itemsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <input type=\"color\" title=\"Stars Color\" name=\"starsColor\" value=\"#76008f\">\n        <input inline type=\"range\" title=\"Item1 Stars\" name=\"card1Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"2\">\n        <input inline type=\"range\" title=\"Item2 Stars\" name=\"card2Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"itemsCount >= 2\">\n        <input inline type=\"range\" title=\"Item3 Stars\" name=\"card3Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"itemsCount >= 3\">\n        <input inline type=\"range\" title=\"Item4 Stars\" name=\"card4Stars\" min=\"1\" max=\"5\" step=\"1\" value=\"5\" condition=\"itemsCount >= 4\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#efefef\" selected>\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=kI1YE_qenSU\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" condition=\"bg.type == 'color'\" checked>\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#c3c3c3\" condition=\"bg.type=='color'&&gradientBg\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row text-center justify-content-center\">\n            <div class=\"col-sm-12 col-md-5 table-wrapper\" mbr-if=\"itemsCount > 0\" mbr-class=\"{'all-border': itemsCount == 1,\n                             'left-bottom': itemsCount >= 2,\n                             'col-lg-6': itemsCount == 2,\n                             'col-lg-4': itemsCount == 3,\n                             'col-lg-3': itemsCount == 4}\">\n                <span class=\"table-heading mbr-fonts-style mbr-white\" data-app-selector=\".table-heading, .stars\" mbr-text mbr-theme-style=\"display-5\">\n                    STANDART\n                </span>\n                <div class=\"stars\" mbr-class=\"{'star1': card1Stars == 1,\n                                 'star2': card1Stars == 2,\n                                 'star3': card1Stars == 3,\n                                 'star4': card1Stars == 4,\n                                 'star5': card1Stars == 5}\">\n                  \t<svg height=\"25\" width=\"23\">\n                      <polygon points=\"9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78\" class=\"rating star-1\"></polygon>\n                    </svg>\n                    <svg height=\"25\" width=\"23\">\n                      <polygon points=\"9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78\" class=\"rating star-2\"></polygon>\n                    </svg>\n                    <svg height=\"25\" width=\"23\">\n                      <polygon points=\"9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78\" class=\"rating star-3\"></polygon>\n                    </svg>\n                    <svg height=\"25\" width=\"23\">\n                      <polygon points=\"9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78\" class=\"rating star-4\"></polygon>\n                    </svg>\n                    <svg height=\"25\" width=\"23\">\n                      <polygon points=\"9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78\" class=\"rating star-5\"></polygon>\n                    </svg>\n                </div>\n                <ul class=\"mbr-fonts-style mbr-list mbr-white\" mbr-text mbr-theme-style=\"display-4\" data-app-selector=\".mbr-list\">\n                  <li>10 Users</li>\n                  <li>100 GB of Disk Space</li>\n                  <li>800 GB Bandwitch</li>\n                  <li>10 Databases</li>\n                  <li>5 Email Accounts</li>\n                </ul>\n                <span class=\"pricing-value mbr-fonts-style mbr-white\" mbr-text mbr-theme-style=\"display-2\" data-app-selector=\".pricing-value, .mbr-section-btn\">S/.20\n                </span>\n                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" data-app-selector=\".mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a href=\"#bottom\" class=\"btn btn-white btn-bgr\" data-app-placeholder=\"Type Text\">COMPRAR</a></div>\n            </div>\n\n            <di